# <%= @name %>
<%- if @application.empty? -%>
<%- @source.each do |fuente| -%>
<%= @action %> <%= fuente %> <%= @dest %> <%= @proto %><%= @port.empty? ? '' : " #{@port}" %><%= @sport.empty? ? '' : " #{@sport}" %><%= @original_dest.empty? ? '' : " #{@original_dest}" %>
<%- end -%>
<%- else -%>
<%= @application %>/<%= @action %> <%= @source %> <%= @dest %><%= @sport.empty? ? '' : " - - #{@sport}" %><%= @original_dest.empty? ? '' : " #{@original_dest}" %>
<%- end -%>
